CREATE TABLE alunos (
	RA_aluno INT PRIMARY KEY,
	nome_aluno VARCHAR(50) NOT NULL,
	sobrenome_aluno VARCHAR(100) NOT NULL,
	mat1_aluno VARCHAR(50) NOT NULL,
	mat2_aluno VARCHAR(50) NULL,
	mat3_aluno VARCHAR(50) NULL

);

CREATE TABLE materias (
	ID_mat INT PRIMARY KEY,
	nome_mat VARCHAR(50) NOT NULL,
	qtd_cred INT NOT NULL

);

CREATE TABLE professores (
	RP_professor INT PRIMARY KEY,
	nome_prof VARCHAR(50) NOT NULL,
	sobrenome_prof VARCHAR(100) NOT NULL,
	materia_dada VARCHAR(50) NOT NULL

);

